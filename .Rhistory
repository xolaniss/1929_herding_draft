source(here("Functions","borderlines.R" ))
set_flextable_defaults(font.size = 8,
text.align = "left",
font.color = "black",
theme_fun = theme_apa,
padding = 0,
line_spacing = 0)
#| data,
#| cache = TRUE
general_herding <- read_rds(here("Outputs", "artifacts_general_herding.rds"))
#| desc,
