if_else(name == "comb_50", "τ = 50%",
if_else(name == "comb_95", "τ = 95%",
if_else(name == "comb_99", "τ = 99%", name)))))) %>%
relocate(tau, .after = name) %>%
dplyr::select(-name, -tau_number) %>%
mutate(across(.col = 2:4, ~str_replace_all(.x, "\\[0]", "[0.0000]")))
